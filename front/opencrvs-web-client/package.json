  "scripts": {
    "test": "karma start",
    "check-coverage": "istanbul check-coverage --statements 23 --branches 5 --functions 9 --lines 24",
    "watch:test": "yarn test -- --auto-watch --no-single-run",
    "validate": "yarn validate-webpack:* lint-staged test --serial check-coverage",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod",
    "build": "webpack --env.dev",
    "build:prod": "webpack --env.prod -p",
    "devserver": "webpack-dev-server --env.dev --open",
    "start": "yarn devserver",
    "precommit": "validate"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "extends": "fbjs"
  }
